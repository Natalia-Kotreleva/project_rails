<p id="notice"><%= notice %></p>

<h1><%= I18n.t('tests.index.header') %></h1>

<%= @tests %>
<%= @tests.inspect %>

<ul><% @tests.each do |test| %>
    <li><%= test.title %></li>
    <% end %>
</ul>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tests.each do |test| %>
      <tr>
        <td><%= test.title %></td>
        <td><%= link_to t('show'), admin_test_path(test) %></td>
        <td><%= link_to t('edit'), edit_admin_test_path(test) %></td>
        <td><%= link_to t('destroy'), admin_test_path(test), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<p><%= link_to t('new'), new_admin_test_path(@test) %></p>
