<h1>Questions#new</h1>

<form action="/tests/<%=params[:test_id]%>/questions" method="post">
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
  <p>
    <label for="question-body">Body:</label>
    <input type="text" name="question[body]" test_id="<%=params[:test_id]%>" id="question-body"
  </p><br>

  <input type="submit" value="Create">
</form>
